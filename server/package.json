{
  "name": "server",
  "version": "1.0.0",
  "description": "Nest MEAN Server",
  "author": "Nartc",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc -p ./tsconfig.json",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "postinstall": "tsc -p ./tsconfig.json",
    "mongo:local": "mongod"
  },
  "dependencies": {
    "@nestjs/common": "5.3.0",
    "@nestjs/core": "5.3.0",
    "@nestjs/mongoose": "5.2.1",
    "@nestjs/passport": "5.0.0",
    "@nestjs/swagger": "2.4.6",
    "@nestjs/websockets": "^5.3.0",
    "automapper-ts": "1.9.0",
    "bcryptjs": "2.4.3",
    "config": "2.0.1",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "mongoose": "5.2.12",
    "nestjs-socket-handlers-with-ack": "^1.1.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.1",
    "typegoose": "5.2.1",
    "typescript": "2.9.2"
  },
  "resolutions": {
    "@types/express": "4.16.0",
    "@types/passport": "0.4.2"
  },
  "devDependencies": {
    "@nestjs/testing": "5.3.0",
    "@types/bcryptjs": "2.4.1",
    "@types/config": "0.0.34",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/jsonwebtoken": "7.2.8",
    "@types/lodash": "4.14.116",
    "@types/mongoose": "5.2.9",
    "@types/node": "10.9.4",
    "@types/passport": "0.4.2",
    "@types/passport-jwt": "3.0.1",
    "@types/supertest": "2.0.5",
    "jest": "23.5.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.2",
    "supertest": "3.1.0",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.5.0",
    "tslint": "5.11.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
